{
   "author": "Phil Grayson",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/icon38-off.png"
      },
      "default_title": "Content-Security-Policy headers are disabled"
   },
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "homepage_url": "https://github.com/PhilGrayson/chrome-csp-disable",
   "icons": {
      "128": "images/icon128.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Always Disable Content-Security-Policy",
   "permissions": [ "webRequest", "webRequestBlocking", "browsingData", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.7"
}
